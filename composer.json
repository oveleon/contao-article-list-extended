{
  "name": "oveleon/contao-article-list-extended",
  "type": "contao-bundle",
  "authors": [
    {
      "name": "Oveleon",
      "homepage": "https://oveleon.de/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || ^5.3",
    "symfony/http-kernel": "^5.4 || ^6.4 || ^7.0",
    "symfony/routing": "^5.4 || ^6.4 || ^7.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Oveleon\\ContaoArticleListExtended\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.1.x-dev"
    },
    "contao-manager-plugin": "Oveleon\\ContaoArticleListExtended\\ContaoManager\\Plugin"
  }
}
